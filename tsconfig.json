{
  "compilerOptions": {
    "target": "es2021",
    "lib": ["es2021"],
    "module": "commonjs",
    "incremental": true,
    "esModuleInterop": true,
    "declaration": true,
    "noImplicitAny": true,
    "removeComments": true,
    "experimentalDecorators": true,
    "rootDir": "./src",
    "baseUrl": "./",
    "outDir": "./dist",
    "sourceMap": true,
    "noEmit": false,
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
  